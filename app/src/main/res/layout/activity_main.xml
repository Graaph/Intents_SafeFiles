<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nicolasschilling.intentsvorlage2.MainActivity">

    <EditText
        android:id="@+id/editText"
        android:layout_width="279dp"
        android:layout_height="266dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="27dp"
        tools:text="Name" />

    <Button
        android:id="@+id/safebtn"
        android:layout_width="305dp"
        android:layout_height="66dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/filesafebtn"
        android:layout_marginTop="29dp"
        android:onClick="ButtonWasClicked"
        android:text="SAFE"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="343dp"
        tools:text="safe" />

    <Button
        android:id="@+id/filesafebtn"
        android:layout_width="305dp"
        android:layout_height="66dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editText"
        android:layout_marginTop="52dp"
        android:onClick="FileSave"
        android:text="FILESAFE"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="343dp"
        tools:text="FILESAFE" />

</RelativeLayout>
